{
    "name": "techdivision/import-app-simple",
    "description": "Pacemaker simple application implementation providing basic Magento 2 import functionality",
    "license": "MIT",
    "require": {
        "php": ">=7.2.0",
        "symfony/console": "~2.3,!=2.7.0|~4.0",
        "techdivision/import": "dev-master"
    },
    "require-dev": {
        "doctrine/dbal": "2.5.*",
        "pdepend/pdepend": "2.5.2",
        "phpmd/phpmd": "@stable",
        "phpunit/phpunit": "~6.5.0",
        "sebastian/phpcpd": "~3.0.0",
        "squizlabs/php_codesniffer": "~3.4.0",
        "consolidation/robo": "~1.0",
        "mikey179/vfsstream": "~1.0",
        "symfony/http-kernel": "~3.4"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Tim Wagner",
            "email": "t.wagner@techdivision.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "TechDivision\\Import\\App\\": [
                "src/",
                "symfony/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TechDivision\\Import\\App\\": [
                "tests/unit/"
            ]
        }
    }
}